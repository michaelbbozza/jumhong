<%=button_to 'Back to Main Page', surveys_path, method: :get%>


<h2><%= @survey.title %></h2><br>


<% @questions.each do |question| %>
  <ul>
    <li> 
    <%= question.content %>
    </li> 
  <%@choices.each do |choice|%>
    <%if choice.question_id == question.id%>
      <%=choice.content%><br>
    <%end%>
  <%end%>
  </ul>
<% end %>



